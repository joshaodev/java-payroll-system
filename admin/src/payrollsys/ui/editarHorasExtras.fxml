<AnchorPane id="AnchorPane" prefHeight="490.0" prefWidth="350.0" stylesheets="@/payrollsys/style/appStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="payrollsys.controllers.EditarHorasExtrasController">
